{
"manifest_version": 2,
    "name": "Politicker",
    "description": "places link for politicians (congress for now)",
    "version": "0.1",
    "icons": {
        "128": "images/USFlag.png"
      },
    "content_scripts": [
        {
            "matches": [
                "*://*/*"
            ],
            "js": [
              "module.js",  
              "content.js"
            ],
            "run_at": "document_end"
        }
    ]
}